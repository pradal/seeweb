{% extends "project_view.html" %}

{% block project_view_body %}

<div class="row">
    <div class="col-md-12">
        {% if project.src_url|length == 0 %}
            <p>No remote documentation associated to this project</p>
        {% else %}
            <a href="{{ project.src_url }}">{{ hostname }}</a>
        {% endif %}
    </div>
</div>

{% endblock %}
