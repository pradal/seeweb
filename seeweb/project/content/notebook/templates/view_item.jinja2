{% macro render_data(mime_type, data) -%}

<div class="output_data_item">
    {% if mime_type == "text/plain" %}
        {% for line in data %}
            {{ line }}<br>
        {% endfor %}
    {% elif mime_type == "image/png" %}
        <img src="data:image/png;base64,{{ data }}" />
    {% endif %}
</div>

{%- endmacro %}

